{%- liquid
    if section.blocks.size > 0 
    assign listWidth = section.settings.content_width | split: '/'
    assign leftColumn = listWidth[0]
    assign rightColumn = listWidth[1]  
    
    assign listWidth = section.settings.lap_content_width | split: '/' 
    assign lap_leftColumn = listWidth[0]
    assign lap_rightColumn = listWidth[1] 
     endif 
-%}

{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }

  @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  }
  .home-custom-section .home-custom-section-wrapper {
    position: relative;
    display: flex;
    flex-wrap: wrap;
    -webkit-box-orient: horizontal;
    -webkit-box-direction: normal;
    -ms-flex-direction: row;
    flex-direction: row;
    background: none;
    align-self: normal;
    min-width: 0;
    word-wrap: break-word;
    background-clip: border-box;
    padding: 0;
    overflow: hidden; gap: 0; justify-content: space-between;
  }
  .home-custom-section .home-custom-section-wrapper .dt-sc-image-gallery { display: grid; grid-template-columns: repeat(2, 1fr);
    gap: var(--DTGutter_Width); gap: {{section.settings.column_gap}}px;
      }
  {% for block in section.blocks %}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.{{block.type}}-{{block.id}} .dt-sc-heading:empty { display: none; }
  {% if block.settings.fullwidth_images %}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.{{block.type}}-{{block.id}} .dt-sc-image-gallery { grid-template-columns: 1fr; }
  {% endif %}
  {% endfor %}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block { display: flex; flex-wrap: wrap; align-self: flex-start; }
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block > * { width: 100%; }
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.dt-sc-reverse-columns { flex-direction: column-reverse; }
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block.dt-sc-reverse-columns .dt-sc-heading {
    margin-top: 50px; margin-bottom: 0; }
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading > *:not(:last-child){margin:0 0 10px;}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{margin-bottom:20px;}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-center{text-align:center;}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-start{text-align:left;}
  .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading.text-end{text-align:right;}
  .home-custom-section{position:relative;overflow:hidden;}
  {% unless section.settings.bg_image == blank %}
  .home-custom-section:before{content:'';background:url('{{ section.settings.bg_image |  image_url: width: 1920 }}');width:100%;height:100%;position:absolute;background-size: cover;background-repeat: no-repeat;}
  {% endunless %}
   
   .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button:after{opacity:0;}
 .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button{background:transparent;border:none;min-height: 80px;min-width: 223px;font-size:18px;color: var(--gradient-base-accent-1); margin-top: 30px;}
 .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button:hover{color: var(--gradient-base-background-2);}
 .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button span.button-icon {
    position: absolute;
    font-size: 244px;
    bottom: 0;
    height: 100%;
    display: flex;
}
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button span.button-icon svg{width:244px;height:auto;}
 .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button span.button-icon svg path{
   stroke-dasharray: 1560;
    stroke-dashoffset: 0;
    fill: transparent;
    stroke-width: 3;
    stroke: rgba(var(--color-base-solid-button-labels));
   }
 .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading a.button:hover span.button-icon svg path{ animation: 2s linear 0s 1 reverse animated-circle;}

@keyframes animated-circle{
0% {
    stroke-dashoffset: 0;
}
40% {
    stroke-dashoffset: 1560;
}
80% {
    stroke-dashoffset: 3120;
}
100% {
    stroke-dashoffset: 3120;
}
}


  
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block > *{width:50%;}
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding-left:150px;padding-right: 10px;}
  .custom-text-image-banner.home-custom-section .dt-sc-image-gallery{display: block;position: relative;}
  .custom-text-image-banner.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:last-child{position: absolute; bottom: 0; z-index: 1;left: 0;
    border-radius: 15px;transform: translateY(160px);-webkit-transform: translateY(160px);}
  .custom-text-image-banner.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:first-child{border-radius: 30px 0px 0px 30px;width: 670px;margin-left: auto;display: flex;}    
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper{padding-bottom:160px;}
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading .dt-sc-sub-heading{
      font-size: 24px;
    font-weight: 400;
    color: rgba(var(--color-base-solid-button-labels));
    margin: 0;
    line-height: normal;}
  .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading .dt-sc-main-heading{
   font-weight: 600;font-size: 60px;}
   .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading .dt-sc-heading-description{color: var(--gradient-base-accent-2);}
   .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading {margin-bottom: 0;}
   .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading > *:not(:last-child){margin-bottom:20px}
    {% for block in section.blocks %}
  .home-custom-section .dt-sc-image-gallery:before{content:'';
    width: 165px;height: 188px;position:absolute;top: 0;left: 70px;background:url('{{ block.settings.before_image | image_url: width: 500 }}');background-repeat:no-repeat;background-size: contain;animation: rotateText 10s linear infinite;} 
    .home-custom-section .dt-sc-image-gallery:after{content:'';
        width: 256px;height: 236px;position:absolute;bottom: -112px;right: 110px;background:url('{{ block.settings.after_image | image_url: width: 500 }}');background-repeat:no-repeat;background-size: contain;} 
   {% endfor %}

@keyframes rotateText {
  0% {
    transform: rotate(0deg);
  }
  50% {
    transform: rotate(50deg);
  }
  100% {
    transform: rotate(0deg);
  }
}
  .about-custom-section.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:first-child{border-radius:30px;}
  .about-custom-section.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding-left:0;} 
  .home-custom-section .home-custom-section-wrapper .custom-menu-icon svg {
    width: 14px;
    height: 14px;fill:rgba(var(--color-base-solid-button-labels));margin-right:15px;
}
  .home-custom-section .home-custom-section-wrapper .custom-menu li{display:flex;align-items: center;}                                                                                                         
  .home-custom-section .home-custom-section-wrapper .custom-menu li:not(:last-child){padding-bottom:6px;}
  .home-custom-section .home-custom-section-wrapper .custom-menu li .list-menu__item--link{color: var(--gradient-base-accent-2);}
  
  @media (min-width: 1541px) {
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:first-child {
      width: calc({{leftColumn}}% - (calc({{section.settings.column_gap}}px)/2)); }
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:last-child {
      width: calc({{rightColumn}}% - (calc({{section.settings.column_gap}}px)/2)); }
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:only-child {
      width: 100%;
    }
    .custom-text-image-banner.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:last-child{left:120px;}
    .about-custom-section.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:last-child{left:0;}
  }
  @media (max-width: 1540px) {
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:first-child {
      width: calc({{lap_leftColumn}}% - (calc({{section.settings.column_gap}}px)/2)); }
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:last-child {
      width: calc({{lap_rightColumn}}% - (calc({{section.settings.column_gap}}px)/2)); }
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:only-child {
      width: 100%;
    }
     .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding-left: 60px;padding-right: 20px;}
     .home-custom-section .dt-sc-image-gallery:after{right:50px;}
     .about-custom-section.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding-left: 0px;}
  }
  @media only screen and (max-width: 1199px) {
    .home-custom-section .home-custom-section-wrapper { display: grid; gap: {{section.settings.column_gap}}px; }
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:first-child,
    .home-custom-section .home-custom-section-wrapper .dt-sc-custom-block:last-child {  width: 100%; }
    
    .home-custom-section .home-custom-section-wrapper .dt-sc-image-gallery { grid-template-columns: 1fr; }
    .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading .dt-sc-main-heading{font-size:40px;}
    
  }
    @media only screen and (max-width: 990px) {
    .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block{flex-direction: column;}
    .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding:0 40px 50px;}
    .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block > *{width:100%;}  
    .about-custom-section.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading{padding: 0 0 50px;}  
    }
    @media only screen and (max-width: 749px) {
      .custom-text-image-banner.home-custom-section .home-custom-section-wrapper .dt-sc-custom-block .dt-sc-heading .dt-sc-main-heading{font-size:30px;}
     .home-custom-section .dt-sc-image-gallery:after {
    right: 30px;
    width: 150px;
    bottom: -82px;
    height: 150px;
     }
    }
    @media only screen and (max-width: 576px) {
      .custom-text-image-banner.home-custom-section .dt-sc-image-gallery .dt-sc-block-image:last-child{width: 100%;height: 100%;object-fit:contain;}
      .home-custom-section .dt-sc-image-gallery:before { width: 100px;height: 100px;}
      .home-custom-section .dt-sc-image-gallery:after {display:none}
    } 
  
{%- endstyle -%}

<div class="home-custom-section color-{{ section.settings.color_scheme }} gradient{% if section.settings.title == blank %} no-heading{% endif %} {{ section.settings.custom_class_name }} ">
 <div class="{% if section.settings.page_full_width %} page-full-width {% else %} page-width {% endif %} {% if section.settings.page_full_width_spacing %} page-full-width_spacing {% endif %} section-{{ section.id }}-padding isolate">
 <div class="row"> 
     {%- unless section.settings.title == blank -%}
      <div class="title-wrapper-with-link title-wrapper--self-padded-mobile title-wrapper--no-top-margin content-align--{{ section.settings.column_alignment }}">
         {%- if section.settings.sub_heading != blank -%}  
         <h6 class="sub-heading">{{ section.settings.sub_heading | escape }}</h6>
         {%- endif -%} 
         {%- if section.settings.title != blank -%}  
          <h2 class="title {{ section.settings.heading_size }}">
            {{ section.settings.title | escape }}
          </h2>
          {%- endif -%} 
          {%- if section.settings.description != blank -%}  
          <p class="description">{{ section.settings.description }}</p>
          {%- endif -%}   
          {%- if section.settings.button_label != blank -%}
            <a {% if section.settings.button_link == blank %} role="link" aria-disabled="true"{% else %} href="{{ section.settings.button_link }}"{% endif %} class="button{% if section.settings.button_style_secondary %} button--secondary{% else %} button--primary{% endif %}">{{ section.settings.button_label | escape }}</a>
          {%- endif -%}
          </div>
    {%- endunless -%}
    <div class="home-custom-section-wrapper">
    {% for block in section.blocks %}
        <div class="dt-sc-custom-block {{block.type}}-{{block.id}} {% if block.settings.reverse_column %}dt-sc-reverse-columns{% endif %}">
          {% if block.settings.block_heading != blank or block.settings.block_sub_heading != blank or block.settings.block_description != blank or block.settings.block_button_text or block.settings.block_button_link != blank %}
          <div class="dt-sc-heading {{ block.settings.heading_position }}">
             {%- if block.settings.block_sub_heading != blank -%}
            <h6 class="dt-sc-sub-heading">{{block.settings.block_sub_heading}}</h6>
            {%- endif -%}
            {%- if block.settings.block_heading != blank -%}
            <h4 class="dt-sc-main-heading">{{block.settings.block_heading}}</h4>
            {%- endif -%}
            {%- if block.settings.block_description != blank -%}
            <p class="dt-sc-heading-description">{{block.settings.block_description}}</p>
            {%- endif -%}
            {%- if block.settings.html != blank -%}
            <div class="dt-sc-heading-html rte">{{block.settings.html}}</div>
            {%- endif -%}
            {%- if block.settings.custom_menu != blank  -%}
                    <ul class="custom-menu list-unstyled ">
                      {%- for link in block.settings.custom_menu.links -%}
                        <li>
                          <span class="custom-menu-icon">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 100 100" style="enable-background:new 0 0 100 100;" xml:space="preserve">
                              <path d="M98,2.4c-1.5-1.7-4-1.9-5.7-0.4c-6.4,6-12.3,12.1-18.1,18.5C68.3,27,62.7,33.5,57.3,40.3c-8.1,10-15.8,20.5-23.1,31.4 L8,42.1c-1.2-1.4-3.4-1.8-5-0.8c-1.9,1.2-2.6,3.8-1.5,5.8c0,0,25,45.3,26,47.2c1,1.9,3.3,4.7,8,4.7c4.5,0,7-2.8,8.5-5.7 c1.5-3,15.4-29.9,24.4-44.2c4.5-7.1,9.2-14.1,14.2-21c4.9-6.8,10.1-13.6,15.4-19.9L98,8.1C99.3,6.5,99.4,4,98,2.4z">
                             </path>
                            </svg>
                          </span>
                          <a href="{{ link.url }}" class="link link--text list-menu__item list-menu__item--link{% if link.active %} list-menu__item--active{% endif %}">
                            {{ link.title }}
                          </a>
                        </li>
                      {%- endfor -%}
                    </ul>
            {%- endif -%}
            {%- if block.settings.block_button_link != blank and block.settings.block_button_text != blank -%}
            <a href="{{block.settings.block_button_link}}" class="button">
               <span class="button-text">{{block.settings.block_button_text}} </span>
            <span class="button-icon">
              <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 242.3 86.6" style="enable-background:new 0 0 242.3 86.6;" xml:space="preserve">
                <style type="text/css">	.btn0{fill:currentcolor;}</style>
                <path class="btn0" d="M135.2,82.7c5.5,0.1,11.4,0.6,16.7,0.3c4.1,0,8,0,12.1-0.5c3.5-0.2,6.6-0.6,9.8-0.9 c2.6,0,5.6-0.5,8.2-0.7c1.5,0,2.1-0.3,5.4-0.5c1.8-0.1,3.6-0.5,5.4-0.5c0.1,0.4-0.3,0.4-0.8,0.5c-3.4,0.3-6.9,0.6-10.3,0.9 c-3.4,0.3-6.7,0.6-10.1,0.9l0,0c-9.9,1.3-23.5,1.3-25.7,1.3c-5.5,0-11-0.1-16.5-0.2c-4.6-0.1-9.3-0.3-13.9-0.6 c-4.6-0.3-9.1-0.7-13.6-1.2c-5.9-0.7-11.8-1.4-17.7-2.3c-7.6-1.2-15.2-2.8-22.7-4.7C55.8,72.7,50.1,71,44.6,69 c-6.7-2.4-13.1-5.5-19.2-9.1c-5.6-3.5-10.5-7.4-13.6-12.1c-1.9-2.9-2.9-5.9-2.5-9.1c0.6-3.5,2.1-6.7,4.5-9.4 c4-4.8,9.8-8.5,16.6-11.6c5-2.2,10.1-4.1,15.3-5.6c5.9-1.7,11.9-3.1,18.1-4.2c6-1.1,12-2,18.2-2.6c6.3-0.6,12.6-1.1,18.9-1.5 c7-0.4,14.1-0.6,21.1-0.4c5.9,0.1,11.8,0.2,17.7,0.6c5.7,0.3,11.5,0.9,17.2,1.5c6.9,0.7,13.6,1.8,20.2,3c5.4,1.1,10.7,2.3,15.9,3.8 l0,0c23.4,6.1,31.6,15.3,31.6,15.3l0,0c0.5,0.5,1,0.9,1.5,1.4c2.2,2.2,4,4.7,5.3,7.5c1.2,2.8,1.8,5.8,1.7,8.9 c-0.1,2.7-1.1,5.3-2.9,7.4c-2.1,2.5-4.7,4.7-7.5,6.5c-5.4,3.4-11.2,6.2-17.3,8.3c-5.2,1.9-10.5,3.5-15.9,4.7 c-6.1,1.4-12.2,2.5-18.4,3.2c-4.3,0.5-8.7,0.8-13.1,0.9c-4.4,0.1-8.9-0.1-13.3-0.7c-4.7-0.5-9.3-1.7-13.7-3.4 c-1-0.4-1.9-0.8-2.7-1.2">
                </path>
              </svg>
            </span></a>
            {%- endif -%}
            
          </div>
          {% endif %}
          {% if block.settings.block_img_1 or block.settings.block_img_2 != blank %}
          <div class="dt-sc-image-gallery">
            {% if block.settings.block_img_1 != blank   %}
            <img
                      class="dt-sc-block-image"
                      srcset="{%- if block.settings.block_img_1.width >= 275 -%}{{ block.settings.block_img_1 | image_url: width: 275 }} 275w,{%- endif -%}
                        {%- if block.settings.block_img_1.width >= 550 -%}{{ block.settings.block_img_1 | image_url: width: 550 }} 550w,{%- endif -%}
                        {%- if block.settings.block_img_1.width >= 710 -%}{{ block.settings.block_img_1 | image_url: width: 710 }} 710w,{%- endif -%}
                        {%- if block.settings.block_img_1.width >= 1420 -%}{{ block.settings.block_img_1 | image_url: width: 1420 }} 1420w,{%- endif -%}
                        {{ block.settings.block_img_1 | image_url }} {{ block.settings.block_img_1.width }}w"
                      src="{{ block.settings.block_img_1 | image_url: width: 550 }}"
                      sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                        (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                        calc(100vw - 30px)"
                      alt="{{ block.settings.block_img_1.alt }}"
                      height="{{ block.settings.block_img_1.height }}"
                      width="{{ block.settings.block_img_1.width }}"
                      loading="lazy"
                    >
            {% endif %}

            {% if block.settings.block_img_2 != blank   %}
            <img
                      class="dt-sc-block-image"
                      srcset="{%- if block.settings.block_img_2.width >= 275 -%}{{ block.settings.block_img_2 | image_url: width: 275 }} 275w,{%- endif -%}
                        {%- if block.settings.block_img_2.width >= 550 -%}{{ block.settings.block_img_2 | image_url: width: 550 }} 550w,{%- endif -%}
                        {%- if block.settings.block_img_2.width >= 710 -%}{{ block.settings.block_img_2 | image_url: width: 710 }} 710w,{%- endif -%}
                        {%- if block.settings.block_img_2.width >= 1420 -%}{{ block.settings.block_img_2 | image_url: width: 1420 }} 1420w,{%- endif -%}
                        {{ block.settings.block_img_2 | image_url }} {{ block.settings.block_img_2.width }}w"
                      src="{{ block.settings.block_img_2 | image_url: width: 550 }}"
                      sizes="(min-width: 990px) {% if section.blocks.size <= 2 %}710px{% else %}550px{% endif %},
                        (min-width: 750px) {% if section.blocks.size == 1 %}710px{% else %}550px{% endif %},
                        calc(100vw - 30px)"
                      alt="{{ block.settings.block_img_2.alt }}"
                      height="{{ block.settings.block_img_2.height }}"
                      width="{{ block.settings.block_img_2.width }}"
                      loading="lazy"
                    >
            {% endif %}
          </div>
          {% endif %}
        </div>
        {% endfor %}
  </div>
 </div>
 </div>
</div>
{% schema %}
{
  "name": "t:sections.custom-section.name",
  "class": "section section-custom-section",
  "max_blocks":2,
  "tag": "section",
  "settings": [
    {
      "type":"checkbox",
      "id":"page_full_width",
       "default": false,
      "label": "t:sections.all.page_full_width.label"
    },
    {
      "type":"checkbox",
      "id":"page_full_width_spacing",
       "default": false,
      "label": "t:sections.all.page_full_width_spacing.label"
    },
    {
    "type": "image_picker",
    "id": "bg_image",
